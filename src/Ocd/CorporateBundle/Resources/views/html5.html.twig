{% block all %}<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7 {{bp_html_classes|default('')}}" lang="{{bp_language|default('en')}}" {{bp_html_attributes|default('')|raw}}><![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8 {{bp_html_classes|default('')}}" lang="{{bp_language|default('en')}}" {{bp_html_attributes|default('')|raw}}><![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9 {{bp_html_classes|default('')}}" lang="{{bp_language|default('en')}}" {{bp_html_attributes|default('')|raw}}><![endif]-->
<!--[if gt IE 8]><!--><html class="no-js {{bp_html_classes|default('')}}" lang="{{bp_language|default('en')}}" {{bp_html_attributes|default('')|raw}}><!--<![endif]-->
{% block head_outer %}
    <head {{bp_head_attributes|default('')|raw}}>
        {% block head %}
            {% block head_meta %}
                <meta charset="utf-8">
                <meta http-equiv="X-UA-Compatible" content="IE=edge">
                {% block head_meta_viewport_tag %}<meta name="viewport" content="{% block head_meta_viewport_tag_content %}width=device-width, initial-scale=1{% endblock %}">{% endblock %}
                <meta name="description" content="{% block head_meta_description %}{% endblock %}">
                <meta name="keywords" content="{% block head_meta_keywords %}{% endblock %}">
            {% endblock head_meta %}

            <title>{% block head_title %}{% endblock %}</title>

            {% block head_css %}
				{% stylesheets 
				'bundles/ocdcorporate/css/html5_boilerplate/html5_boilerplate.css'
				'bundles/ocdcorporate/css/bootstrap/bootstrap.min.css'
				'bundles/ocdcorporate/css/bootstrap/bootstrap-responsive.min.css'
				'bundles/ocdcorporate/css/html5_boilerplate/font-awesome.min.css'
				'bundles/ocdcorporate/css/main_layout.css'
				 %}
					<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
				{% endstylesheets %}
            {% endblock %}
            {% block head_js %}
					<script src="{{ asset('bundles/ocdcorporate/js/html5_boilerplate/modernizr.js') }}"></script>
            {% endblock %}
        {% endblock head %}
    </head>
{% endblock head_outer %}
{% block body_outer %}
    <body {{bp_body_attributes|default('')|raw}}>
    {%block body%}
        {% block body_chromeframe %}
            <!--[if lt IE 7]>
                <p class="chromeframe">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">activate Google Chrome Frame</a> to improve your experience.</p>
            <![endif]-->
        {% endblock body_chromeframe %}
        {%block body_container%}
            <div id="container">
                <header id="header">
					<section class="logo_container">
						<div class="cube">
							<figure class="front"><p>O</p></figure>
							<figure class="left"><p><i class="fa fa-qrcode"></i></p></figure>
							<figure class="right"><p>C</p></figure>
							<figure class="top"><p><i class="fa fa-cloud"></i></p></figure>
							<figure class="bottom"><p><i class="fa fa-search"></i></p></figure>
							<figure class="back"><p>D</p></figure>
						</div>
					</section>
					<section class="title_container">
                        <h1><a href="{{ url('homepage_corporate') }}" title="homepage">Open Code Development</a></h1>
						{% block body_container_header %}{% endblock %}
					</section>
					<div id="block_login">
						{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
							<div id="block_login" class="dropdown">
								<a href="{{ path('fos_user_profile_show') }}" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-user"></i></a>
								<div class="dropdown-menu">
									
									{% if is_granted("ROLE_ADMIN") %}
										<p>
											<ul>
												<li><a href="{{ path('sonata_admin_dashboard') }}" title="Admin">Admin</i></a></li>
												<li><a href="{{ path('sonata_user_profile_show') }}" title="Profil">Profil</a></li>
											</ul>
										</p>
									{% else %}
										<p>coucou</p>
									{% endif %}
								</div>
							</div>
							<a href="{{ path('fos_user_security_logout') }}" title="Logout from {{ app.user.username }}"><i class="fa fa-sign-out"></i></a>
						{% else %}
							<a href="{{ path('fos_user_security_login') }}"><i class="fa fa-sign-in"></i></a>
						{% endif %}
					</div>
					<nav class="navbar navbar-inverse page__nav" role="navigation">
						<div class="container-fluid">
							{{ knp_menu_render('main', {'template': 'OcdCorporateBundle:Menu:bootstrap.html.twig', 'currentClass': 'active'}) }}

							<ul class="nav  navbar-nav  navbar-right  navbar--external">
								<li><a href="http://cmf.symfony.com/">Website</a></li>
								<li><a href="http://symfony.com/doc/current/cmf/">Documentation</a></li>
								<li><a href="http://github.com/symfony-cmf">Github</a></li>
							</ul>
						</div>
					</nav>
                </header>
                <div role="main" class="container">
                    {% block body_container_main %}{% endblock %}
                </div>
                <footer>
                    {% block body_container_footer %}{% endblock %}
                </footer>
            </div>
        {%endblock body_container%}
        {% block body_js %}
			{% javascripts '@OcdCorporateBundle/Resources/public/js/html5_boilerplate/jquery-2.1.4.min.js' %}
				<script type="text/javascript" src="{{ asset_url }}"></script>
			{% endjavascripts %}
			{% javascripts
				'@OcdCorporateBundle/Resources/public/js/bootstrap/bootstrap.js'
				'@OcdCorporateBundle/Resources/public/js/corporate.js' %}
				<script src="{{ asset_url }}"></script>
			{% endjavascripts %}
            {% block body_js_analytics %}
                {% if bp_analytics_id|default(none) %}
                    <script>
                        (function(b,o,i,l,e,r){b.GoogleAnalyticsObject=l;b[l]||(b[l]=
                        function(){(b[l].q=b[l].q||[]).push(arguments)});b[l].l=+new Date;
                        e=o.createElement(i);r=o.getElementsByTagName(i)[0];
                        e.src='//www.google-analytics.com/analytics.js';
                        r.parentNode.insertBefore(e,r)}(window,document,'script','ga'));
                        var gao=gao||{};
                        {% if bp_analytics_domain|default(none) %}gao.cookieDomain='{{ bp_analytics_domain }}';{% endif %}
                        {% block body_js_analytics_extra %}{% endblock %}
                        ga('create','{{ bp_analytics_id }}',gao);{% block body_js_analytics_track %}ga('send','pageview');{% endblock %}
                    </script>
                {% endif %}
            {% endblock body_js_analytics %}
        {% endblock body_js %}
    {% endblock body %}
    </body>
{% endblock body_outer %}
</html>{% endblock all %}
